<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.HBox?>

<fx:root styleClass="tasksItem" type="BorderPane" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1">
	<center>
		<StackPane styleClass="cardBg">
			<children>
				<HBox fx:id="card" prefHeight="1.0" prefWidth="130.0" styleClass="card">
   <children>
                  <StackPane alignment="CENTER_LEFT" prefHeight="27.0" prefWidth="178.0">
                     <children>
      							<Text fx:id="filename" strokeWidth="0.0" styleClass="filename" text="Filename" wrappingWidth="275.7958984375">
                           <font>
                              <Font size="7.0" />
                           </font>
                        </Text>
                     </children>
                  </StackPane>
                  <Region HBox.hgrow="ALWAYS" />
                  <StackPane prefHeight="27.0" prefWidth="0.0">
                     <children>
                        <Rectangle fx:id="tagDateColor" arcHeight="5.0" arcWidth="5.0" fill="WHITE" height="27.0" stroke="TRANSPARENT" strokeType="INSIDE" width="52.0" />
                        <Text fx:id="date" fill="#1743f2" stroke="WHITE" strokeType="OUTSIDE" strokeWidth="0.0">
                           <font>
                              <Font name="AR ESSENCE" size="18.0" />
                           </font>
                           <effect>
                              <Glow level="0.4" />
                           </effect>
                        </Text>
                        <Label fx:id="labelDate" text="Label" textFill="WHITE">
                           <font>
                              <Font name="System Bold" size="10.0" />
                           </font>
                        </Label>
                     </children>
                  </StackPane>
   </children>
				</HBox>
			</children>
		</StackPane>
	</center>
   <effect>
      <DropShadow color="#43a0e0" />
   </effect>
   <left>
      <Rectangle fx:id="circle" arcHeight="5.0" arcWidth="5.0" fill="DODGERBLUE" height="55.0" stroke="TRANSPARENT" strokeType="INSIDE" styleClass="circle" width="10.0" BorderPane.alignment="CENTER" />
   </left>
</fx:root>
